<div class="head">
    <div id="logo"><a href="/"><i class="fab fa-opencart"></i></a><a style="display: none;" href="/">Keby<span>.</span></a></div>
    <form action="">
        <div>
            <input type="search" name="search" id="search">
            <button type="submit">search</button>
        </div>
    </form>
    <ul>
        <% if (session.isAuth) { %>
            <li>welcome <%= session.user.username %>  </li>
            <li><a href="/auth/logout">logout</a></li>
        <% } else { %>
            <li><a href="/auth"><i class="fas fa-user-circle"></i></a></li>
        <% } %>
        <li><a href=""><i class="fas fa-heart"></i></a></li>
        <% if (session.cart) { %>
            <li><a href=""><i class="fas fa-shopping-cart"></i></a><span><%= session.cart.totalItems %> </span></li>
        <% } else { %>
            <li><a href=""><i class="fas fa-shopping-cart"></i></a></li>
        <% } %>
    </ul>
</div>